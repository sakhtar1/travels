<% @country.visits.each do |visit| %>
	<% if visit.visit_places.present? %>

		<h5 class="visit">Place Visited: <%= link_to visit.visit_places, country_visit_path(visit.country, visit) %></h5>
			
	    <h5>Date: <%= visit.visit_date.strftime("%m/%d/%Y")%></h5>
	 
	   
	    
	    <%= button_to "Delete", country_visit_path(visit.country, visit), :method => :delete, :class => "destroy" %>
	    
    	<br>
    	<br>
    <% end %>
  <% end %>
