<h5><% @buckets.each do |bucket| %>
	<% if bucket.country.present? %>
		<label><input type="checkbox" name="checkbox" id="outside" class="sum" value="10" data-toggle="checkbox"><strong> <%= link_to bucket.country, bucket %></strong></label>
		<a href="#" class="js-see_more" data-id="<%=@bucket.id%>">(See More...)</a>
		 <%#= button_to "Delete", bucket, :method => :delete, :class => "destroy" %>
		  <br>
	 <% end %>
	
	
<%end %>
</h5>



